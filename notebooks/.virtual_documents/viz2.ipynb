dq12_tensor_dx[:, 0] = (q_tensor[:, 1, 0, 1] - q_tensor[:, 0, 0, 1]) / dx
dq12_tensor_dx[:, -1] = (q_tensor[:, -1, 0,1] - q_tensor[:, -2, 0, 1]) / dx

