      q_tensor[i,j,0,0] = 1.5 *(S_i * S_i - 0.5)
      q_tensor[i,j,0,1] = 1.5 *(S_i * S_j)
      q_tensor[i,j,1,0] = 1.5 *(S_i * S_j)
