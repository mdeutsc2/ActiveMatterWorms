def find_midpoint(x1,y1,x2,y2):
    xm = (x1 + x2) / 2
    ym = (y1 + y2) / 2
    return xm,ym

def calculate_distance(x1, y1, x2, y2):
    # Compute the difference in x and y coordinates
    dx = x2 - x1
    dy = y2 - y1
    # Calculate the distance using the Euclidean distance formula
    distance = np.sqrt(dx**2 + dy**2)
    return distance

