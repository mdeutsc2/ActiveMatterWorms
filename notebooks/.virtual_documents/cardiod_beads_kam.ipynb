        p2y = pos_data[iframe+i,pairs_list[ipair,1],1]
        distances[ipair,i] = calculate_distance(p1x,p1y,p2x,p2y)

mean_dists = np.sum(distances,axis=0,dtype=np.float64)/distances.shape[0]

